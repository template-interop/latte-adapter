{
    "name": "template-interop/latte-adapter",
    "description": "Latte adapter for template-interop/engine",
    "keywords": [
        "render",
        "template",
        "engine",
        "templating",
        "output",
        "latte"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Keven",
            "email": "hello@keven.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "template-interop/engine": "^0.0.2",
        "latte/latte": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Interop\\Template\\Latte\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Interop\\Template\\Latte\\": "src/"
        }
    }
}
